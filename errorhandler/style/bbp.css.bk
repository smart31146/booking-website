
/* TRAINING  */
.t_menu_blue {
	width:300px;
	height:36px;
	-moz-border-radius: 7px;
	border-radius: 7px;
	margin-top:6px;
	background:#DBEEFF url('../images/g_arrow_menu_white.png') no-repeat  96% center;
}
.t_menu_blue_active {
	width:300px;
	height:36px;
	-moz-border-radius: 7px;
	border-radius: 7px;
	margin-top:6px;
	background:#9ED2FF url('../images/g_arrow_menu_blue.png') no-repeat  96% center;
}

.t_choose {
	width:260px;
	margin-right:15px;
	float:left;
	-moz-border-radius: 7px;
	border-radius: 7px;
	margin-top:6px;
	background:#FFF;
}
.quiz_choose {
	-moz-border-radius: 7px;
	border-radius: 7px;
	background:#FFF;
}
.t_choose_active {
	width:260px;
	margin-right:15px;
	float:left;
	-moz-border-radius: 7px;
	border-radius: 7px;
	margin-top:6px;
	background:#0B66BD ;
}
.box_inside a:link, .box_inside a:visited {	color:#0354AC;	text-decoration:underline;}
.quiz_choose_inside {	display:block;color:#1E89D9;text-decoration:none;font-weight:normal;padding:6px 15px;font-size:16px;}
.t_choose a:link, .t_choose a:visited {	display:block;color:#1E89D9;text-decoration:none;font-weight:normal;padding:10px;font-size:16px;}
.t_choose_active a:link, .t_choose_active a:visited {	display:block;color:#FFF;text-decoration:none;font-weight:normal;padding:10px;font-size:16px;}


.t_menu_blue a:link, .t_menu_blue a:visited {	display:block;color:#1E89D9;text-decoration:none;font-weight:normal;padding:8px 0px 0px 25px;font-size:16px;}

.t_menu_blue_active a:link, .t_menu_blue_active a:visited {	display:block;color:#1E89D9;text-decoration:none;font-weight:bold;padding:8px 0px 0px 25px;font-size:16px;}
.t_menu_blue a:active, .t_menu_blue a:hover,.t_menu_blue_active a:active, .t_menu_blue_active a:hover {	display:block;text-decoration:underline;}


.t_content_info {
	float:left;
	width:320px;
	background:#0154AD url('../images/bg_div_info.gif') repeat-x ;
	min-height:390px;
}
.t_content_info_inside {
	color:#FFF;
