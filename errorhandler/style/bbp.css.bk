/* CLASSER */
.rounded  {
	-webkit-border-radius: 7px;
	-moz-border-radius: 7px;
	border-radius: 7px;
}

.txtField {
	padding: 4px;
	font-size:14px;
	font-weight:bold;
	padding:5px 0px 5px 10px;
	outline: none;
	font-family:arial;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
	border: 1px solid #DFDFDF;
	width:170px;
	-webkit-transition: .3s ease-in-out;
	-moz-transition: .3s ease-in-out;
	-o-transition: .3s ease-in-out;
}
.txtField:focus {
	border: 1px solid #DFDFDF;
	-webkit-box-shadow: 0px 0px 3px #003771;
	-moz-box-shadow: 0px 0px 3px #003771;
	box-shadow: 0px 0px 3px #003771;
}
.bgdot_h  {
background: url('../bilder/bg_dot_h.png') repeat-x 0px 14px;
margin:0px 0px;
padding:14px 0px;
}
.clear {clear: both;width: 100%;height:0px;line-height:0px;font-size:0px;padding:0px;margin:0px;border: 0;}
ul.ulsquare {list-style: square outside;color:#999999;padding:0px;}
ul.ulsquare li span {color:#353535;}
ul.ulsquare li span a {color:#353535;font-weight:normal;text-decoration:none;}

