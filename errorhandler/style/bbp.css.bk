	font-size:20px;
}

/* HTML */
html
{

	width: 100%;
	height: 100%;
	margin: 0px;
	margin-top: 0px;
	text-align: center;
	font-family: Arial,Verdana;
	color: #353535;
}

ul.square {list-style-type: square;margin:5px 5px;padding-left:20px;line-height:1.4em;} 
form	{display: inline;}
img		{border: 0px;}
select	{font-size: 12px; font-family: Arial, Verdana, "Trebuchet MS", sans-serif; } 
input 	{font-family: Arial, Verdana, "Trebuchet MS", sans-serif;}
div, body
{
	font-family: Arial, Verdana, "Trebuchet MS", Sans-Serif;
	font-weight: normal;
	font-size: 14px;
	color: #353535;
	line-height: 1.3em;
	empty-cells: show;
}
body { margin-top:0px; }

table, tr, td
{
	font-family: Arial, Verdana, "Trebuchet MS", Sans-Serif;
	font-weight: normal;
	font-size: 14px;
	line-height: 1.3em;
	empty-cells: show;
}

/* LINKS  */
a:link,a:visited	{color: #000000;font-weight:bold;}
a:hover,a:active	{color: #3e4852;}
.h1active a:link,.h1active a:visited{color:#FFF;text-shadow:1px 1px 0px #999999;}

/* MENU  */
ul.menu {
list-style-type: none;
height: 45px;
width: 920px;
margin: 0px;
padding-left:10px;
}
ul.menu a {
padding-right: 25px;
padding-left: 25px;
display: block;
line-height: 45px;
text-decoration: none;
font-family: Arial;
font-size:12px;
color: #1E89D9;
}
ul.menu a:link,ul.menu a:visited {
color: #77B1E7;
}
ul.menu a:hover {
color: #FFF;
}
ul.menu li {
float: left;
}
/* FONTS  */
h1 {
	font-size: 16px;
	line-height: 1.3em;
	font-weight: bold;
	font-family: arial;
	color: #0354AC;
	padding: 0px 0px 0px 0px;
	margin: 0px 0px 6px 0px;
}
h2 {
	font-size: 16px;
	line-height: 1.3em;
	font-weight: bold;
	font-family: arial;
	color: #4B4B4B;
	padding: 0px 0px 0px 0px;
	margin: 0px 0px 6px 0px;
}

h3 {
	font-size: 26px;
	line-height: 1.2em;
	font-weight: bold;
	font-family: arial;
	color: #0354AC;
	padding: 0px 0px 0px 0px;
	margin: 0px 0px 12px 0px;
}
.box_text_blue {
	color: #0354AC;
	font-size:16px;
}

